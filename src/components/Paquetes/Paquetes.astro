<div class="paquetes-section">
  <div class="paquetes-header">
    <h2 class="t-stroke t-shadow">Paquetes</h2>
    <p>
      Al adquirir nuestros servicios de esta forma obtenés descuentos especiales
    </p>
  </div>

  <!-- Imagen activa y fantasma para transición fade -->
  <img
    id="paquetes-img"
    class="paquetes-img"
    src="https://th.bing.com/th/id/OIP.veOfD2wxZ9M7bRpUsbbIsAAAAA?rs=1&pid=ImgDetMain"
  />
  <img id="ghost-img" class="paquetes-img ghost" src="" />

  <div class="paquetes-list">
    <a
      href="#"
      data-img="https://th.bing.com/th/id/OIP.yvo_eG8Tu5nfew87rdRAKAHaGf?w=206&h=181&c=7&r=0&o=5&pid=1.7"
      >Gastronómicos</a
    >
    <a
      href="#"
      data-img="https://www.bing.com/images/search?view=detailV2&ccid=kHF%2b84w0&id=9600A4B3A154F596CE90FFD5FF613827027D71DD&thid=OIP.kHF-84w03yFKTYnRUeNAqgHaDt&mediaurl=https%3a%2f%2ffjwp.s3.amazonaws.com%2fblog%2fwp-content%2fuploads%2f2020%2f10%2f02072937%2fFreelancer-start.png&cdnurl=https%3a%2f%2fth.bing.com%2fth%2fid%2fR.90717ef38c34df214a4d89d151e340aa%3frik%3d3XF9Aic4Yf%252fV%252fw%26pid%3dImgRaw%26r%3d0&exph=800&expw=1600&q=Freelancers&simid=607989408244523239&FORM=IRPRST&ck=5F32C8995683AF3189424534DFFA1855&selectedIndex=0&itb=0"
      >Freelancers</a
    >
    <a
      href="#"
      data-img="data:image/jpeg;base64,"
      >Portafolios</a
    >
    <a
      href="#"
      data-img="https://th.bing.com/th/id/OIP.D-5XLBO5FKFYU-w7DjIIIgHaE8?w=272&h=181&c=7&r=0&o=5&pid=1.7"
      >Consultorios</a
    >
    <a
      href="#"
      data-img="data:image/jpeg;base64,"
      >Autónomos</a
    >
  </div>

  <a class="btn-mod modifier">+ Paquetes</a>
</div>

<style>
  .paquetes-section {
    color: var(--background);
    padding: 0% 5%;
    height: 90vh;
    display: flex;
    flex-direction: column;
    justify-content: space-around;
    position: relative;
  }
  


  .paquetes-header h2 {
    font-size: 2rem;
    margin: 0;
    font-weight: 500;
  }

  .paquetes-header p {
    font-size: 0.9rem;
    margin-top: 1rem;
    line-height: 1.5;
    width: 100%;
  }

  .paquetes-img {
    position: absolute;
    top: 30%;
    right: 5%;
    width: 300px;
    height: 200px;
    aspect-ratio: 3 / 2;
    object-fit: cover;
    border-radius: 3px;
    transition: opacity 0.5s ease;
    z-index: -1;
  }

  .ghost {
    opacity: 0;
    position: absolute;
    z-index: 0;
    pointer-events: none;
  }

  .paquetes-list {
    display: flex;
    flex-direction: column;
    justify-content: center;
    align-items: flex-start;
    z-index: 2;
  }

  .paquetes-list a {
    padding: 1rem 0;
    color: var(--background);
    text-decoration: none;
    font-weight: 400;
    width: 70%;
    position: relative;
    padding-bottom: 5px;
    font-size: 1.618rem;
    text-transform: uppercase;
    border-bottom: 2px solid var(--background);
    transition: background-color 0.3s;
  }

  .paquetes-list a:hover {
    background-color: white;
    color: #1d1ddb;
  }
  @media (min-width: 1280px) {
    .paquetes-section {
      padding: 10% 5%;
      height: 100vh;
      display: flex;
      flex-direction: column;
      justify-content: space-around;
      position: relative;
  }
  


  .paquetes-header h2 {
    font-size: 4rem;
    margin-top: 50px;
    font-weight: 300;
  }

  .paquetes-header p {
    font-size: 2rem;
    margin-top: 1rem;
    line-height: 1.5;
    font-weight: 200;
    width: 40%;
  }

  .paquetes-img {
    position: absolute;
    top: 25%;
    right: 10%;
    width: 50vw;
    height: calc(50vw / 1.618);
    aspect-ratio: 3 / 2;
    object-fit: cover;
    border-radius: 3px;
    transition: opacity 0.5s ease;
    z-index: -1;
  }

  .ghost {
    opacity: 0;
    position: absolute;
    z-index: 0;
    pointer-events: none;
  }

  .paquetes-list {
    display: flex;
    flex-direction: column;
    justify-content: center;
    align-items: flex-start;
    z-index: 2;
  }

  .paquetes-list a {
    padding: 1rem 0;
    color: var(--background);
    text-decoration: none;
    font-weight: 400;
    width: 50%;
    position: relative;
    padding-bottom: 5px;
    font-size: calc(1.618rem*2);
    text-transform: uppercase;
    border-bottom: 1px solid var(--background);
    transition: background-color 0.3s;
  }

  .paquetes-list a:hover {
    background-color: white;
    color: #1d1ddb;
  }
  .modifier{
    display: flex;
    justify-content: center;
    align-items: center;
    width: 20%;
  }
  }
  
</style>

<script type="module">
  document.addEventListener("DOMContentLoaded", () => {
    const mainImg = document.getElementById("paquetes-img");
    const ghostImg = document.getElementById("ghost-img");
    const links = document.querySelectorAll(".paquetes-list a");
    const defaultImg = mainImg.src;

    links.forEach((link) => {
      const imgSrc = link.getAttribute("data-img");

      link.addEventListener("mouseenter", () => {
        ghostImg.src = imgSrc;
        ghostImg.style.opacity = 1;

        setTimeout(() => {
          mainImg.src = imgSrc;
          ghostImg.style.opacity = 0;
        }, 400);
      });

      link.addEventListener("mouseleave", () => {
        ghostImg.src = defaultImg;
        ghostImg.style.opacity = 1;

        setTimeout(() => {
          mainImg.src = defaultImg;
          ghostImg.style.opacity = 0;
        }, 400);
      });
    });
  });
</script>

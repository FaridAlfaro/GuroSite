<style>
   header {
      min-height: 100vh;
      display: flex;
      flex-direction: column;
      justify-content: center;
      align-items: center;
      overflow-x: hidden;
      transition: ease 0.5s;
      animation: ease 0.5s;
   }
   .logo-container {
      height: 100dvh;
      width: 100%;
      display: flex;
      justify-content: center;
      align-items: center;
      transition: ease 0.5s;
      animation: ease 0.5s;
      margin-bottom: 60px;
   }

   .video {
      position: absolute;
      z-index: -1;
      height: 100vh;
      width: 100%;
      object-fit: cover;
      transition: ease 0.5s;
      animation: ease 0.5s;
   }
</style>
